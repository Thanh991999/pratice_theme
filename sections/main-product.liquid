              {%- endif -%}
            </div>
          {%- when 'popup_content' -%}
            {%- liquid
              assign popup_content_dialog_id = block.id | append: section.id | md5
              assign popup_content_block = block
            %}
            <div class="product__info product__popup-content p-sides" {{ block.shopify_attributes }}>
              {%- if block.settings.static_page != blank -%}
