                  {% endif %}
                  <div class="info-card__body hide-empty rte">{{- block.settings.tertiary_content -}}</div>
                </div>
                <div class="product__extra-info--quaternary info-card hide-empty">
                  {% if block.settings.quaternary_heading != blank %}
                    <div class="info-card__heading mb-12 d-flex fd-column hide-empty">
                      {%- render "icon-loader", class: block.settings.color_scheme, size: 36, icon: block.settings.quaternary_options_icon -%}
                      <span class="hdg-5">{{- block.settings.quaternary_heading -}}</span>
                    </div>
                  {% endif %}
                  <div class="info-card__body hide-empty rte">{{- block.settings.quaternary_content -}}</div>
                </div>
              {% endif %}
