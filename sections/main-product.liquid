{{ 'section-main-product.css' | asset_url | stylesheet_tag: preload: true }}
{%- liquid
  assign variant_label = 'color,colour,couleur,colore,boja,farbe,색,色,warna,dath,боја,lliw,liw,колір,värvi,värv,գույն,krāsa,màu sắc,màu,cor,cцвят,färg,farve,ngjyrë,kleur,väri,farge,renk,culoare,szín,цвет,kolor,barva,χρώμα,litur,spalva,farba' | split: ','
  assign place_holder = 'product-1' | placeholder_svg_tag: 'placeholder js-image-lazy w-100 h-100 p-absolute'
  assign rand = "now" | date: "%N" | md5
  assign is_first_media = false
  assign is_first_image = false
  assign product_form_id = 'productForm-' | append: section.id
  assign sticky_product_form_id = 'productStickyForm-' | append: section.id
  assign product_form_installment_id = 'productFormInstallment-' | append: section.id
  assign first_3d_model = product.media | where: "media_type", "model" | first
  assign size_guide_block = nil
  assign size_guide_dialog_id = ''
  assign popup_content_block = nil
  assign popup_content_dialog_id = ''
  assign nav_mobile = 'd-none-important'
  assign pg_mobile = 'd-none-important'
  if section.settings.enable_pagination_mobile
    assign pg_mobile = 'd-flex'
  endif
  if section.settings.enable_navigation_mobile
    assign nav_mobile = 'd-flex'
  endif
  assign product_card_media = settings.product_card_image_ratio
  assign thumb_index = 0
%}
{%- if first_3d_model -%}
  <script type="application/json" id="ProductJSON-{{ product.id }}">{{ product.media | where: 'media_type', 'model' | json }}</script>
{%- endif -%}
<div class="ctnr d-none-md">{%- render 'breadcrumb' -%}</div>
<div class="main-product section" id="{{ section.id }}" data-ui-component="Main Product Section">
  <div class="product-details main-product-detail p-relative zi-1">
    <div class="product__media-wrapper">
      {%- if section.settings.desktop_media_layout == 'single_with_thumbnails' -%}
        <link rel="stylesheet" href="{{ 'horizontal-carousel.css' | asset_url }}" media="all">
        <div class="product__media-wrapper--single-with-thumbnails p-sticky top-0">
          <sht-product-slideshow class="product-slideshow p-relative"  data-hide-variant-image="true" data-section="{{ section.id }}">
            <div class="product-slideshow__thumbnail d-none d-block-md no-js-hidden">
              <sht-vertical-carousel class="carousel--vertical" data-section="{{ section.id }}">
                <div class="product-slideshow__thumbs js-carousel-items vertical-carousel vertical-carousel__container d-flex fd-column p-zero m-zero oy-auto p-relative disable-scrollbars fw-nowrap scrollable scrollable-y scrl-bhvr-auto" role="list" aria-label="{{ 'accessibility.slider' | t }}">
                  {%-liquid
                    if product.selected_or_first_available_variant.featured_media != null
                      assign thumb_index = thumb_index | plus: 1
                      assign featured_media = product.selected_or_first_available_variant.featured_media
                      assign is_first_media = true
                      render 'product-slideshow-thumb', section: section, media: featured_media, product: product, place_holder: place_holder, is_first_media: is_first_media, thumb_index: thumb_index
                    endif
                    for media in product.media
                      assign thumb_index = thumb_index | plus: 1
                      unless media.id == product.selected_or_first_available_variant.featured_media.id
                        if forloop.first and is_first_media == false
                          assign is_first_media = true
                        else
                          assign is_first_media = false
                        endif
                        render 'product-slideshow-thumb', section: section, media: media, product: product, place_holder: place_holder, is_first_media: is_first_media, thumb_index: thumb_index
                      endunless
                    endfor
                  -%}
                </div>
                <div class="product-slideshow__pagination carousel-controls--outside  carousel-pagination d-none center-xs no-js-hidden js-carousel-pagination">
                  <button type="button" data-step="1" class="button button--primary button--small button--icon button--prev js-carousel-prev-btn" aria-label="{{ 'accessibility.previous_slide' | t }}" name="previous">
                    {% render 'icon-chevron-left', width: 24, height: 24 %}
                  </button>
                  <button type="button" data-step="1" class="button button--primary button--small button--icon button--next js-carousel-next-btn" aria-label="{{ 'accessibility.next_slide' | t }}" name="next">
                    {% render 'icon-chevron-right', width: 24, height: 24 %}
                  </button>
                </div>
              </sht-vertical-carousel>
            </div>
            <div class="product-slideshow__slider p-relative">
              {% render 'product-gallery-carousel', product: product, section: section, place_holder: place_holder, rand: rand, hide_variant_image: true, is_first_image: is_first_image, open_zoom_modal: section.settings.enable_media_zoom, pg_mobile: pg_mobile, nav_mobile: nav_mobile %}
            </div>
          </sht-product-slideshow>
        </div>
      {%- endif -%}
      {%- if section.settings.desktop_media_layout == 'gallery_grid' -%}
        <div class="product__media-wrapper--gallery-grid" id="productMediaGalleryGrid-{{ section.id }}">
          <sht-product-slideshow class="product-slideshow" data-hide-variant-image="false" data-section="{{ section.id }}">
            <div class="product-slideshow__slider p-relative">
              {% render 'product-gallery-carousel', product: product, section: section, place_holder: place_holder, rand: rand, hide_variant_image: false, is_first_image: is_first_image, open_zoom_modal: section.settings.enable_media_zoom, pg_mobile: pg_mobile, nav_mobile: nav_mobile %}
            </div>
          </sht-product-slideshow>
        </div>
      {%- endif -%}
    </div>
    <div class="product__info-wrapper">
      <div id="productInfo-{{ section.id }}" class="main-product-info-container product__info-container d-flex fd-column gap-36">
        <div class="d-flex-md d-none">{%- render 'breadcrumb' -%}</div>
        {%- for block in section.blocks -%}
          {%- case block.type -%}
          {%- when '@app' -%}
            {% render block %}
          {%- when 'product_meta' -%}
            <div class="product__info product__meta p-sides" {{ block.shopify_attributes }}>
              {%- if block.settings.show_vendor_name -%}
                <div class="color--light fs-14"><span class="visually-hidden">{{ 'accessibility.vendor' | t }}</span><span>{{ product.vendor }}</span></div>
              {%- endif -%}
              <div class="product__title d-flex middle-xs between-xs">
                <h1 class="hdg-2 m-zero mr-8">{{ product.title | escape }}</h1>
                <a href="{{ product.url }}" class="d-none"><h2>{{ product.title | escape }}</h2></a>
                {%- if block.settings.enable_share -%}
                  <sht-share-product-button class="product__share" data-product-url="{{ product.selected_variant.url | default: product.url | prepend: request.origin }}">
                    <div class="js-web-share-api-container" hidden>
                      <button class="button button--share button--icon button--small button--secondary js-web-share-api-button" aria-label="{{ 'accessibility.social_share' | t }}">{% render 'icon-share' %}</button>
                    </div>
                    <sht-details-exposal>
                      <details class="js-none-web-share-api-container p-relative zi-2" id="productShareDetails-{{ block.id }}-{{ section.id }}">
                        <summary aria-controls="productShare-{{ block.id }}-{{ section.id }}" aria-expanded="false" class="button button--small ls-none button--icon button button--secondary button--share js-none-web-share-api-button">
                          {% render 'icon-share', pros: 'aria-hidden="true" focusable="false"' %}
                        </summary>
                        <div id="productShare-{{ block.id }}-{{ section.id }}" class="p-absolute social-share-list">
                          {% render 'product-social-share', product: product %}
                        </div>
                      </details>
                    </sht-details-exposal>
                  </sht-share-product-button>
                  {%- liquid
                    assign files = "share-product-button.js" | split: ', '
                    render 'js-loader', files: files
                  -%}
                {%- endif -%}
              </div>
              {%- if block.settings.enable_product_rating -%}
                {% render 'product-rating', product: product %}
              {%- endif -%}
            </div>
          {%- when 'basic_description' -%}
            <div class="product__info product__description p-sides rte" {{ block.shopify_attributes }}>
              <span class="d-block mb-16 hdg-5">{{ 'products.product.description' | t }}</span>
              {{ product.description }}
            </div>
          {%- when 'variants_and_price' -%}
            <div class="product__info product__variants-and-price d-flex fd-column" {{ block.shopify_attributes }}>
              <div class="product__price-and-extrainfo">
                <div class="product__price no-js-hidden d-flex top-xs fw-wrap" id="productPrice-{{ section.id }}" role="status">
                  {% render 'product-price', product: product, use_variant: true, show_badges: true, price_class: 'price--large', price_sale_bold: 'fw-700' %}
                  {% if block.settings.show_sale_badge %}
                    {%- liquid
                    if product.selected_or_first_available_variant.compare_at_price > product.selected_or_first_available_variant.price
                      assign on_sale = true
                      assign discount_cost = product.selected_or_first_available_variant.compare_at_price | minus: product.selected_or_first_available_variant.price
                      assign discount_percent = discount_cost | times: 100.0 | divided_by: product.selected_or_first_available_variant.compare_at_price | round: 2
                    endif
                    -%}
                    {% if on_sale %}
                    <div class="product__badge mt-8 mb-8">
                      <span class="sale-badge fs-sm d-inline-block p-relative fw-700">
                        <span class="p-relative zi-1 tt-up">{{ 'products.product.on_sale' | t }}
                          {% if settings.currency_code_enabled %}
                            {{- discount_cost | money_with_currency -}}
                          {% else %}
                            {{- discount_cost | money -}}
                        {% endif %}
                        ({{ discount_percent }}%)
                      </span>
                      </span>
                    </div>
                    {% endif %}
                  {% endif %}
                </div>
                {%- if block.settings.show_tax_and_shipping_policy -%}
                  <div class="product__extrainfo hide-empty fs-sm">
                    {%- if shop.taxes_included -%}
                      <span class="product__tax-info">
                        {{- 'products.product.include_taxes' | t -}}
                      </span>
                    {%- endif -%}
                    {%- if shop.shipping_policy.body != blank -%}
                      <span class="product__shipping-policy-info">
                        {{- 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url -}}
                      </span>
                    {%- endif -%}
                  </div>
                {% endif %}
                {%- form 'product', product, id: product_form_installment_id, class: 'installment caption-large' -%}
                  <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                  {{ form | payment_terms }}
                {%- endform -%}
              </div>
              {%- unless product.has_only_default_variant -%}
                <div class="product__variants p-sides" id="variantPicker-{{ section.id }}">
                  <sht-variant-radios class="product__variants--radios no-js-hidden" data-section="{{ section.id }}" data-url="{{ product.url }}">
                    {%- for option in product.options_with_values -%}
                      {% assign index = forloop.index | minus: 1 %}
                      <fieldset class="form__fields product-option js-variant-radio-container d-flex fw-wrap p-zero m-zero b-zero">
                        <legend class="p-zero m-zero product-option__name">
                          <span class="hdg-6 mr-8">{{ option.name }}</span>
                          <span class="product-option__name--selected js-variant-radio-option-name color--light hide-empty" data-option-name="{{ option.name }}">
                            {{ product.selected_or_first_available_variant.options[index] }}
                          </span>
                        </legend>
                        {%- assign variant_name = option.name | downcase -%}
                        {%- if variant_label contains variant_name and settings.enable_variant_swatches -%}
                          {% render 'main-product-color-swatch', product: product, option: option, section: section, product_form_id: product_form_id %}
                        {%- else-%}
                          <ul class="product-option-list ls-none d-flex fw-wrap">
                          {%- for value in option.values -%}
                            <li class="form__field js-variant-option-value product-option__value {% if product.selected_or_first_available_variant.available == false and option.selected_value == value %}product-option--sold-out{% endif %}" data-value="{{ value | escape }}">
                                <input type="radio" class="form__control input--variant js-variant-radio-item js-variant-item visually-hidden" id="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}"
                                  name="{{ option.name }}"
                                  value="{{ value | escape }}"
                                  data-variant-value="{{ value | escape }}"
                                  form="{{ product_form_id }}"
                                  {% if option.selected_value == value %}checked{% endif %} />
                                <label class="form__label product-option__label js-variant-option-label" for="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}">{{ value }}</label>
                            </li>
                          {%- endfor -%}
                        </ul>
                        {%- endif -%}
                      </fieldset>
                    {%- endfor -%}
                    <script type="application/json">
                      {{ product.variants | json }}
                    </script>
                  </sht-variant-radios>
                </div>
                <noscript>
                  <div class="form__fields {% if product.has_only_default_variant %} hidden{% endif %}">
                    <div class="form__field">
                      <label class="form__label" for="Variants-{{ section.id }}-{{ forloop.index0 }}">{{ 'products.product.product_variants' | t }}</label>
                      <select name="id" id="Variants-{{ section.id }}-{{ forloop.index0 }}" class="form__control w-100 h-100 field__text field--focus field--hover" form="{{ product_form_id }}">
                        {%- for variant in product.variants -%}
                          <option
                            {% if variant == product.selected_or_first_available_variant %}selected="selected"{% endif %} {% if variant.available == false %}disabled{% endif %} value="{{ variant.id }}">
                            {{ variant.title }}{%- if variant.available == false %} - {{ 'products.product.sold_out' | t }}{% endif %}- {{ variant.price | money | strip_html }}
                          </option>
                        {%- endfor -%}
                      </select>
                    </div>
                  </div>
                </noscript>
              {%- endunless -%}
              {% if block.settings.show_stock_number %}
                <div class="product__inventory hide-empty p-sides" id="inventoryTracking-{{ section.id }}">
                  {%- if product.selected_or_first_available_variant.inventory_quantity > 0 -%}
                    {%- liquid
                      assign stock_status = 'product--instock'
                      if settings.product_information_low_stock_threshold > 0 and settings.product_information_low_stock_threshold >= product.selected_or_first_available_variant.inventory_quantity
                          assign stock_status = 'product--lowstock'
                      endif
                    -%}
                  <span class="{{ stock_status }}">
                    {%- if block.settings.show_stock_number -%}
                        {{ 'products.product.in_stock_html' | t: stock: product.selected_or_first_available_variant.inventory_quantity }}
                      {%- else -%}
                      {{ 'products.product.in_stock' | t }}
                    {%- endif -%}
                  </span>
                  {%- else -%}
                    {%- if product.selected_or_first_available_variant.inventory_policy != 'continue' and product.selected_or_first_available_variant.inventory_management != blank -%}
                    <span class="product--outstock">
                      <b>{{ 'products.product.out_of_stock' | t }}</b>
                    </span>
                    {%- endif -%}
                  {%- endif -%}
                </div>
              {% endif %}
            </div>
          {%- when 'purchasing' -%}
            <sht-sticky-product-info class="product__info d-block" data-section-id="{{ section.id }}" data-is-enabled="{{ section.settings.show_sticky_purchasing_bar }}">
              <div class="product__purchasing js-sticky-product-info p-sides {{ section.settings.primary_button }}" {{ block.shopify_attributes }}>
                <div class="product__form">
                  <sht-product-form class="sht-product__form" data-section="{{ section.id }}">
                    <div class="form--error js-product-form-error-wrapper" data-type="error-wrapper" role="alert" hidden>
                      <span class="d-flex middle-xs">
                        <span class="icon--error icon icon--small mr-8 flx-auto">
                        {%- render 'icon-error', size: 16 -%}
                        </span>
                        <span class="js-product-form-error-message" data-type="error-message"></span>
                      </span>
                    </div>
                    {%- form 'product', product, id: product_form_id, class: 'form form__fieds', novalidate: 'novalidate', data-type: 'add-to-cart-form' -%}
                      <div class="form__fied">
                        <input type="hidden" class="form__control" name="id" value="{{ product.selected_or_first_available_variant.id }}" disabled>
                      </div>
                      <div class="product-form__buttons {% if block.settings.show_dynamic_checkout_buttons %} dynamic_button_show {% else %} dynamic_button_hide {% endif %} d-flex top-xs fd-column">
                        <div class="product-form__buttons__quantity d-flex w-100">
                          <div class="product__quantity{% if block.settings.show_quantity_selector == 'never' or block.settings.show_quantity_selector == 'add-to-cart' %} d-none{% endif %}" id="productQuantity-{{ section.id }}" data-show="{{ block.settings.show_quantity_selector }}">
                            <label class="field__label visually-hidden" for="productQuantityInput-{{ section.id }}">{{ 'products.product.quantity.label' | t }}:</label>
                            <sht-quantity-input class="field_quantity">
                              <button class="no-js-hidden js-quantity-button js-quantity-button--minus button button--icon button--large button--plain" aria-label="{{ 'accessibility.minus' | t }}" name="minus" data-name="minus" type="button">
                                {%- render 'icon-minus', size: 20 -%}
                              </button>
                              <input class="js-quantity-input qty-element input-number b-zero ta-center"
                                  type="number"
                                  name="quantity"
                                  aria-label="{{ 'products.product.quantity.input_label' | t: product: product.title | escape }}"
                                  id="productQuantityInput-{{ section.id }}"
                                  min="1"
                                  value="1"
                                  form="{{ product_form_id }}"/>
                                <button class="no-js-hidden js-quantity-button js-quantity-button--plus button button--icon button--large button--plain" aria-label="{{ 'accessibility.plus' | t }}" name="plus" data-name="plus" type="button">
                                  {%- render 'icon-plus', size: 20 -%}
                                </button>
                            </sht-quantity-input>
                          </div>
                          <button type="submit" name="add" class="js-product-form-submit-btn product-button--atc button button--secondary button--large w-100" {% if product.selected_or_first_available_variant.available == false %}disabled{% endif %}>
                            <span class="p-relative d-flex middle-xs center-xs ta-center">
                              <span class="js-product-form-spinner p-absolute icon">
                                {% render 'icon-spinner', class: 'spinner' %}
                              </span>
                                <span class="js-product-form-submit-btn-text">
                                  {%- if product.selected_or_first_available_variant.available -%}
                                    {{ 'products.product.add_to_cart' | t }}
                                  {%- else -%}
                                    {{ 'products.product.sold_out' | t }}
                                  {%- endif -%}
                              </span>
                            </span>
                          </button>
                        </div>
                          <sht-lazy-load-dynamic-checkout-button>
                            <template>
                              {{ form | payment_button }}
                            </template>
                          </sht-lazy-load-dynamic-checkout-button>
                      </div>
                    {%- endform -%}
                  </sht-product-form>
                </div>
              </div>
            </sht-sticky-product-info>
          {%- when 'extra_information' -%}
            <div class="product__info product__extra-info product__extra-info--{{- block.settings.display_style }} p-sides" {{ block.shopify_attributes }}>
            {% if block.settings.display_style == 'collapsibles' %}
              {% capture accordion_icons %}
                <span class="zi-1 button button--small button--icon">
                  {% render "icon-plus", class: 'accordion__icon--plus p-absolute' %}
                  {% render "icon-minus", class: 'accordion__icon--minus p-absolute' %}
                </span>
              {% endcapture %}
                <sht-accordion data-properties='{"multiple": true}'>
                {%- if block.settings.primary_heading != blank -%}
                  <div class="accordion__item js-accordion-item">
                    <div class="accordion__header">
                      <a href="#pac-primary--{{ forloop.index }}-{{- block.id -}}" role="button" class="accordion__trigger js-accordion-trigger d-flex middle-xs p-relative pt-16 pb-16" id="accordion-primary-title-{{- block.id -}}" aria-expanded="false" aria-controls="accordion-primary-content-{{- block.id -}}">
                        {%- render "icon-loader", class: block.settings.color_scheme, size: 36, icon: block.settings.primary_options_icon -%}
                        <span class="accordion__title hdg-5 m-zero fg-1 ml-12">
                          {{ block.settings.primary_heading }}
                        </span>
                        <span class="accordion__toggle zi-1 d-flex">{{ accordion_icons }}</span>
                      </a>
                    </div>
                    <span id="pac-primary--{{ forloop.index }}-{{ block.id }}" hidden></span>
                    <div class="js-accordion-content pb-24 pl-48 rte pac-primary--{{ forloop.index }}-{{ block.id }}" id="accordion-primary-content-{{- block.id -}}" role="region" aria-describedby="product__extra-info-{{ block.id }}" tabindex="-1" hidden>
                      {{- block.settings.primary_content -}}
                    </div>
                    <noscript>
                      <style>
                        #pac-primary--{{ forloop.index }}-{{ block.id }}:target ~ .pac-primary--{{ forloop.index }}-{{ block.id }} {
                          display: block !important;
                        }
                      </style>
                    </noscript>
                  </div>
                {% endif %}
                {%- if block.settings.secondary_heading != blank -%}
                  <div class="accordion__item js-accordion-item">
                    <div class="accordion__header">
                      <a href="#pac-secondary--{{ forloop.index }}-{{- block.id -}}" role="button" class="accordion__trigger js-accordion-trigger d-flex middle-xs p-relative pt-16 pb-16" id="accordion-secondary-title-{{- block.id -}}" aria-expanded="false" aria-controls="accordion-secondary-content-{{- block.id -}}">
                        {%- render "icon-loader", class: block.settings.color_scheme, size: 36, icon: block.settings.secondary_options_icon -%}
                        <span class="accordion__title hdg-5 m-zero fg-1 ml-12">
                          {{ block.settings.secondary_heading }}
                        </span>
                        <span class="accordion__toggle zi-1 d-flex">
                          {{ accordion_icons }}
                        </span>
                      </a>
                    </div>
                    <span id="pac-secondary--{{ forloop.index }}-{{- block.id -}}" hidden></span>
                    <div class="js-accordion-content pb-24 pl-48 rte pac-secondary--{{ forloop.index }}-{{- block.id -}}" id="accordion-secondary-content-{{- block.id -}}" role="region" aria-describedby="product__extra-info-{{ block.id }}" tabindex="-1" hidden>
                      {{- block.settings.secondary_content -}}
                    </div>
                    <noscript>
                      <style>
                        #pac-secondary--{{ forloop.index }}-{{ block.id }}:target ~ .pac-secondary--{{ forloop.index }}-{{ block.id }} {
                          display: block !important;
                        }
                      </style>
                    </noscript>
                  </div>
                {% endif %}
                {%- if block.settings.tertiary_heading != blank -%}
                  <div class="accordion__item js-accordion-item">
                    <div class="accordion__header">
                      <a href="#pac-tertiary--{{ forloop.index }}-{{- block.id -}}" role="button" class="accordion__trigger js-accordion-trigger d-flex middle-xs p-relative pt-16 pb-16" id="accordion-tertiary-title{{- block.id -}}" aria-expanded="false" aria-controls="accordion-tertiary-content-{{- block.id -}}">
                        {%- render "icon-loader", class: block.settings.color_scheme, size: 36, icon: block.settings.tertiary_options_icon -%}
                        <span class="accordion__title hdg-5 m-zero fg-1 ml-12">
                          {{ block.settings.tertiary_heading }}
                        </span>
                        <span class="accordion__toggle zi-1 d-flex">
                          {{ accordion_icons }}
                        </span>
                      </a>
                    </div>
                    <span id="pac-tertiary--{{ forloop.index }}-{{- block.id -}}" hidden></span>
                    <div class="js-accordion-content pb-24 pl-48 rte pac-tertiary--{{ forloop.index }}-{{- block.id -}}" id="accordion-tertiary-content-{{- block.id -}}" role="region" aria-describedby="product__extra-info-{{ block.id }}" tabindex="-1" hidden>
                      {{- block.settings.tertiary_content -}}
                    </div>
                    <noscript>
                      <style>
                        #pac-tertiary--{{ forloop.index }}-{{ block.id }}:target ~ .pac-tertiary--{{ forloop.index }}-{{ block.id }} {
                          display: block !important;
                        }
                      </style>
                    </noscript>
                  </div>
                {% endif %}
                {%- if block.settings.quaternary_heading != blank -%}
                  <div class="accordion__item js-accordion-item">
                    <div class="accordion__header">
