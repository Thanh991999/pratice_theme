          {%- when 'pickup_availability' -%}
          {%- assign pick_up_availabilities = product.selected_or_first_available_variant.store_availabilities | where: 'pick_up_enabled', true -%}
          {% if pick_up_availabilities.size > 0 %}
            <div class="product__info product__pickup-availability" {{ block.shopify_attributes }}>
              <sht-pickup-availability class="pickup-availability d-block no-js-hidden color-accent-1 style--paper-note"
                {% if product.selected_or_first_available_variant.available and pick_up_availabilities.size > 0 %} available {% endif %}
                data-base-url="{{ shop.url }}{{ routes.root_url }}"
                data-variant-id="{{ product.selected_or_first_available_variant.id }}"
                data-has-only-default-variant="{{ product.has_only_default_variant }}"
              >
                <template>
                  <sht-pickup-availability-preview class="d-flex top-xs pickup-availability__preview">
                    {% render 'icon-check-mark' %}
                    <div class="pickup-availability__info">
                      <p>{{ 'products.product.pickup_availability.unavailable' | t }}</p>
                      <button class="button button-small button-plain">{{ 'products.product.pickup_availability.refresh' | t }}</button>
                    </div>
                  </sht-pickup-availability-preview>
                </template>
              </sht-pickup-availability>
              {%- liquid
                assign files = "pickup-availability.js" | split: ', '
                render 'js-loader', files: files
              -%}
            </div>
          {% endif %}
          {%- when 'product_cross_sell' -%}
            <div class="product__info product__cross-sell cross-sell" {{ block.shopify_attributes }}>
              <div class="cross-sell__heading">
                <h2 class="d-block hdg-5 p-sides mb-24">
                  {{ block.settings.heading }}
                </h2>
                <div class="cross-sell__items rw">
                  {%- for cross_sell in block.settings.product_list -%}
                    <div class="cross-sell__item clm" style="--w-xs: 6;--w-sm: 6;--w-md: 6;">
                      {% render 'card-product', product: cross_sell, show_rating: false, show_vendor: true, show_color_swatch: false, show_sale_badge: true, enable_quick_buy: false, item_per_row: 2, ratio: product_card_media, is_first: forloop.first, card_content_inner: false %}
                    </div>
                  {%- endfor -%}
                </div>
              </div>
            </div>
          {%- when 'custom_text' -%}
            <div class="product__info product__custom-text p-sides hide-empty {{ block.settings.text_style | replace: 'color-light', 'color--light' | replace: 'body', 'color-text-regular' }} " {{ block.shopify_attributes }}>{{- block.settings.text -}}</div>
          {%- when 'custom_liquid' -%}
            <div class="product__info product__custom-liquid p-sides hide-empty" {{ block.shopify_attributes }}>{{- block.settings.liquid_code -}}</div>
          {%- when 'size_guide' -%}
            {%- liquid
              assign size_guide_dialog_id = block.id | append: section.id | md5
              assign size_guide_block = block
            %}
            <div class="product__info product__size-guide p-sides" {{ block.shopify_attributes }}>
              {%- if block.settings.static_page != blank or block.settings.product_metafield_name != blank -%}
                <button class="button button--large button--link" type="button" aria-haspopup="dialog" onclick="window.dialogSizeGuide{{ size_guide_dialog_id }}.showModal(this, true)">{{- block.settings.heading -}}{% render 'icon-loader', icon: block.settings.size_guide_icon, size: 20, class: 'ml-8' %}</button>
