                      {%- render "icon-loader", class: block.settings.color_scheme, size: 36, icon: block.settings.secondary_options_icon -%}
                    <span class="hdg-5">{{- block.settings.secondary_heading -}}</span>
                  </div>
                  {%- endif -%}
                  <div class="info-card__body hide-empty rte">{{- block.settings.secondary_content -}}</div>
                </div>
                <div class="product__extra-info--tertiary info-card hide-empty">
                  {% if block.settings.tertiary_heading != blank %}
                    <div class="info-card__heading mb-12 d-flex fd-column hide-empty">
                      {%- render "icon-loader", class: block.settings.color_scheme, size: 36, icon: block.settings.tertiary_options_icon -%}
